<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD SQL Map mapper2.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dpInqMapperblessing">
	<!-- 账户查询-->
	<select id="dpInqMapper"  resultType="Map" parameterType="Map" >
		SELECT *			
		FROM
			dp_dpacct
		WHERE
			`acct_nbr` = #{acct_nbr}
	</select>
</mapper>